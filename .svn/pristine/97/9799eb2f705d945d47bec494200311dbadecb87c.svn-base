<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
  <form-beans>
    <form-bean name="phoneInfoForm" type="com.wtwd.sys.phoneinfo.form.PhoneInfoForm" />
    <form-bean name="playItemInfoForm" type="com.wtwd.sys.playiteminfo.form.PlayItemInfoForm" />
    <form-bean name="settingInfoForm" type="com.wtwd.sys.settinginfo.form.SettingInfoForm" />
    <form-bean name="relativeCallInfoForm" type="com.wtwd.sys.relativecallinfo.form.RelativeCallInfoForm" />
    <form-bean name="appUserInfoFrom" type="com.wtwd.sys.appuserinfo.form.AppUserInfoForm" />
    <form-bean name="locationInfoForm" type="com.wtwd.sys.locationinfo.form.LocationInfoForm" />
    <form-bean name="deviceActiveInfoForm" type="com.wtwd.sys.deviceactiveinfo.form.DeviceActiveInfoForm" />
    <form-bean name="checkInfoForm" type="com.wtwd.sys.checkinfo.form.CheckInfoForm" />
    <form-bean name="feedBackInfoForm" type="com.wtwd.sys.feedbackinfo.form.FeedBackInfoForm" />
    <form-bean name="shareInfoForm" type="com.wtwd.sys.shareinfo.form.ShareInfoForm" />
    <form-bean name="msgInfoForm" type="com.wtwd.sys.msginfo.form.MsgInfoForm" />
    <form-bean name="safeAreaForm" type="com.wtwd.sys.safearea.form.SafeAreaForm" />
    <form-bean name="companyInfoForm" type="com.wtwd.sys.companyinfo.form.CompanyInfoForm"></form-bean>
    <form-bean name="channelInfoForm" type="com.wtwd.sys.channelinfo.form.ChannelInfoForm"></form-bean>
    <form-bean name="projectInfoForm" type="com.wtwd.sys.projectinfo.form.ProjectInfoForm"></form-bean>
    <form-bean name="saleInfoForm" type="com.wtwd.sys.saleinfo.form.SaleInfoForm"></form-bean>
    <form-bean name="deviceLoginForm" type="com.wtwd.sys.deviceLogin.form.DeviceLoginForm"></form-bean>
    <form-bean name="userLoginInfoForm" type="com.wtwd.sys.userlogininfo.form.UserLoginInfoForm"></form-bean>
    
    <!-- wappusers -->
    <form-bean name="wappusersFrom" type="com.wtwd.sys.innerw.wappusers.form.WappUsersForm"></form-bean>
  </form-beans>
	<global-forwards>
    	<forward contextRelative="true" name="error" path="/include/errormsg.jsp" />
		<forward contextRelative="true" name="result" path="/public/Result.jsp" />
		<forward contextRelative="true" name="abouts" path="About.htm" />
	</global-forwards>

	<action-mappings>
		<action name="appUserInfoFrom" path="/dyconfig/appUserInfo/doAppUserInfo" scope="request" type="com.wtwd.sys.appuserinfo.action.AppUserInfoAction" parameter="method">
			<forward name="queryAppUserInfo" path="/dyconfig/appUserInfo/appUserInfoMain.jsp" />
    		<forward name="queryAppUserCount" path="/dyconfig/appUserInfo/appUserCountMain.jsp" />
    	</action>
    	
    	<action name="deviceActiveInfoForm" path="/dyconfig/deviceActiveInfo/doDeviceActiveInfo" scope="request" type="com.wtwd.sys.deviceactiveinfo.action.DeviceActiveInfoAction" parameter="method">
			<forward name="queryDeviceActiveInfo" path="/dyconfig/deviceActiveInfo/deviceActiveInfoMain.jsp" />
    		<forward name="queryDeviceActiveCount" path="/dyconfig/deviceActiveInfo/deviceActiveCountMain.jsp" />
    		<forward name="queryDeviceActiveHistory" path="/dyconfig/deviceActiveInfo/deviceActiveHistoryMain.jsp"></forward>
    	</action>
    	
    	<action name="locationInfoForm" path="/dyconfig/locationInfo/doLocationInfo" scope="request" type="com.wtwd.sys.locationinfo.action.LocationInfoAction" parameter="method">
			<forward name="queryLocationInfo" path="/dyconfig/locationInfo/locationInfoMain.jsp" />
    	</action>
    	
    	<action name="phoneInfoForm" path="/dyconfig/phoneInfo/doPhoneInfo" scope="request" type="com.wtwd.sys.phoneinfo.action.PhoneInfoAction" parameter="method">
			<forward name="queryPhoneInfo" path="/dyconfig/phoneInfo/phoneInfoMain.jsp" />
			<forward name="queryPhoneIMEIInfo" path="/dyconfig/phoneInfo/phoneIMEIInfoMain.jsp"></forward>
    		<forward name="addimei" path="/dyconfig/phoneInfo/addImei.jsp"></forward>
    		<forward name="manualInput" path="/dyconfig/phoneInfo/manualInputImei.jsp"></forward>
    		<forward name="excelInput" path="/dyconfig/phoneInfo/excelInputImei.jsp"></forward>
			<forward name="textInput" path="/dyconfig/phoneInfo/textInputImei.jsp"></forward>
    	</action>
    	
    	<action name="checkInfoForm" path="/dyconfig/checkInfo/doCheckInfo" scope="request" type="com.wtwd.sys.checkinfo.action.CheckInfoAction" parameter="method">
			<forward name="queryCheckInfo" path="/dyconfig/checkInfo/checkInfoMain.jsp" />
    	</action>
    	
    	<action name="feedBackInfoForm" path="/dyconfig/feedBackInfo/doFeedBackInfo" scope="request" type="com.wtwd.sys.feedbackinfo.action.FeedBackInfoAction" parameter="method">
			<forward name="queryFeedBackInfo" path="/dyconfig/feedBackInfo/feedBackInfoMain.jsp" />
    	</action>
    	
    	<action name="relativeCallInfoForm" path="/dyconfig/relativeCallInfo/doRelativeCallInfo" scope="request" type="com.wtwd.sys.relativecallinfo.action.RelativeCallInfoAction" parameter="method">
			<forward name="queryRelativeCallInfo" path="/dyconfig/relativeCallInfo/relativeCallInfoMain.jsp" />
    	</action>
    	
    	<action name="shareInfoForm" path="/dyconfig/shareInfo/doShareInfo" scope="request" type="com.wtwd.sys.shareinfo.action.ShareInfoAction" parameter="method">
    		<forward name="queryShareInfo" path="/dyconfig/shareInfo/shareInfoMain.jsp" />
    	</action>
    	
    	<action name="msgInfoForm" path="/dyconfig/msgInfo/doMsgInfo" scope="request" type="com.wtwd.sys.msginfo.action.MsgInfoAction" parameter="method">
    		<forward name="queryMsgInfo" path="/dyconfig/msgInfo/msgInfoMain.jsp" />
    	</action>
    	
    	<action name="safeAreaForm" path="/dyconfig/safeArea/doSafeArea" scope="request" type="com.wtwd.sys.safearea.action.SafeAreaAction" parameter="method">
    		<forward name="querySafeArea" path="/dyconfig/safeArea/safeAreaMain.jsp" />
    	</action>
    	
    	<action name="settingInfoForm" path="/dyconfig/settingInfo/doSettingInfo" scope="request" type="com.wtwd.sys.settinginfo.action.SettingInfoAction" parameter="method">
    		<forward name="querySettingInfo" path="/dyconfig/settingInfo/settingInfoMain.jsp" />
    	</action>
    	<action name="companyInfoForm" path="/dyconfig/companyInfo/doCompanyInfo" scope="request" type="com.wtwd.sys.companyinfo.action.CompanyInfoAction" parameter="method">
			<forward name="queryCompanyInfo" path="/dyconfig/companyInfo/companyInfoMain.jsp" />
   	 		<forward name="addCompanyInfo" path="/dyconfig/companyInfo/addCompanyInfo.jsp"></forward>
   	 	</action>
   	 	
   	 	<action name="channelInfoForm" path="/dyconfig/channelInfo/doChannelInfo" scope="request" type="com.wtwd.sys.channelinfo.action.ChannelInfoAction" parameter="method">
   	 		<forward name="queryChannelInfo" path="/dyconfig/channelInfo/channelInfoMain.jsp"></forward>
   	 		<forward name="addChannelInfo" path="/dyconfig/channelInfo/addChannelInfo.jsp"></forward>
   	 	</action>
   	 	
   	 	<action name="projectInfoForm" path="/dyconfig/projectInfo/doProjectInfo" scope="request" type="com.wtwd.sys.projectinfo.action.ProjectInfoAction" parameter="method">
   	 		<forward name="queryProjectInfo" path="/dyconfig/projectInfo/projectInfoMain.jsp"></forward>
   	 		<forward name="addProjectInfo" path="/dyconfig/projectInfo/addProjectInfo.jsp"></forward>
   	 	</action>
   	 	
   	 	<action name="saleInfoForm" path="/dyconfig/saleInfo/doSaleInfo" scope="request" type="com.wtwd.sys.saleinfo.action.SaleInfoAction" parameter="method">
   	 		<forward name="querySaleInfo" path="/dyconfig/saleInfo/saleInfoMain.jsp"></forward>
   	 		<forward name="querySaleAreaInfo" path="/dyconfig/saleInfo/saleAreaMain.jsp"></forward>
   	 		<forward name="queryDayAddInfo" path="/dyconfig/saleInfo/dayAddMain.jsp"></forward>
   	 		<forward name="queryProductInfo" path="/dyconfig/saleInfo/productInfoMain.jsp"></forward>
   	     	<forward name="insertProductInfo" path="/dyconfig/saleInfo/productInfoAdd.jsp" />
   	     	<forward name="updateProductInfo" path="/dyconfig/saleInfo/productInfoUpdate.jsp" />
   	          <forward name="iconLook" path="/dyconfig/saleInfo/iconLookInfo.jsp"></forward>
   	 	</action>
   	 	
   	 	<action name="userLoginInfoForm" path="/dyconfig/userLoginInfo/doUserLoginInfo" scope="request" type="com.wtwd.sys.userlogininfo.action.UserLoginInfoAction" parameter="method">
   	 		<forward name="queryUserLoginInfo" path="/dyconfig/userLoginInfo/userLoginInfoMain.jsp"></forward>
   	 	</action>
   	 	
   	 	<action name="deviceLoginForm" path="/dyconfig/deviceActiveInfo/doDeviceLogin" scope="request" type="com.wtwd.sys.deviceLogin.action.DeviceLoginAction" parameter="method">
   	 		<forward name="queryDeviceLogin" path="/dyconfig/deviceLogin/deviceLogin.jsp"></forward>
   	 	</action>

	    <!-- wappusers -->
		<action name="wappusersFrom" path="/dyconfig/wappUsers/doWappUsers" scope="request" type="com.wtwd.sys.innerw.wappusers.action.WappUsersAction" parameter="method">
			<forward name="queryWappUsers" path="/dyconfig/wappUsers/wappUsersMain.jsp" />
    		<forward name="queryWappUsersCount" path="/dyconfig/wappUsers/wappUsersCountMain.jsp" />
    	</action>


	</action-mappings>
	
    <message-resources parameter="ApplicationResources" />
</struts-config>

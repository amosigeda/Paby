		<action path="/doGetFall" type="com.wtwd.sys.appinterfaces.doGetFallAction"></action>
		<action path="/REMOTE" type="com.wtwd.sys.appinterfaces.doRemoteAction"></action>
		<action path="/SET_DISTURB" type="com.wtwd.sys.appinterfaces.doDisturbAction"></action>
		<action path="/GET_LOWELECTRICITY" type="com.wtwd.sys.appinterfaces.doGetLowAction"></action>
		<action path="/SET_CLOCK" type="com.wtwd.sys.appinterfaces.doClockAction"></action>
		<action path="/SET_SLEEP" type="com.wtwd.sys.appinterfaces.doSleepAction"></action>
		<action path="/GET_CLOCK" type="com.wtwd.sys.appinterfaces.getClockAction"></action>
		<action path="/GET_SLEEP" type="com.wtwd.sys.appinterfaces.getSleepAction"></action>
		<action path="/GET_DISTURB" type="com.wtwd.sys.appinterfaces.getDisturbAction"></action>
		<action path="/GET_PHONE" type="com.wtwd.sys.appinterfaces.getPhoneAction"></action>
		<action path="/QUERYTRACECOUNT" type="com.wtwd.sys.appinterfaces.getTraceCountAction"></action>
		<action path="/doSendMedia" type="com.wtwd.sys.appinterfaces.userSendMediaAction"></action>
		<action path="/doGetMedia" type="com.wtwd.sys.appinterfaces.userGetMediaAction"></action>
		<action path="/doDeleteShareDevice"
			type="com.wtwd.sys.appinterfaces.DoDeleteShareDeviceAction"></action>
		<action path="/upLoadSportData" type="com.wtwd.sys.appinterfaces.UploadStepData"></action>

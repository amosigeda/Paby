		<action path="/doSimOperate" type="com.wtwd.sys.appinterfaces.doSimOperateAction"></action>
		<action path="/doSelectFlowData" type="com.wtwd.sys.appinterfaces.doSeletFlowData"></action>
         <!-- payfor 支付接口 -->
		<action path="/payfor" type="com.wtwd.sys.appinterfaces.payforAction"></action>
        <!-- 取消订阅 -->
		<action path="/cancelSubscription" type="com.wtwd.sys.appinterfaces.cancelSubscriptionAction"></action>
		   <!-- 修改订阅计划 -->
		<action path="/updateSubscription" type="com.wtwd.sys.appinterfaces.weitechao.updatePayforAction"></action>
		<!-- 查看到期日期 -->
		<action path="/queryExpireDate" type="com.wtwd.sys.appinterfaces.queryExpireDateAction"></action>
		<!-- 查询是否有绑定iccid -->
		<action path="/selectBindIccid" type="com.wtwd.sys.appinterfaces.selectBindIccidAction"></action>
        <!-- 负载均衡 设备现在需要使用的接口 -->
        <action path="/deviceEnterServer" type="com.wtwd.sys.appinterfaces.weitechao.DeviceEnterServerAction"></action>		
        <!-- 负载均衡 APP现在需要使用的接口 -->
        <action path="/appSelectDeviceInnetIp" type="com.wtwd.sys.appinterfaces.weitechao.AppSelectDeviceInnetIp"></action>	
        <!-- 查询imei的默认iccid状态接口 -->
        <action path="/doWTAppGpsManIccid" type="com.wtwd.sys.appinterfaces.weitechao.WTAppGpsManAction"></action>
        <!-- 负载均衡设备交互指令接口 -->
		<action path="/doWTDevGW" type="com.wtwd.sys.appinterfaces.innerw.WTDevGWAction"></action>

        <!-- 查询设备状态接口 -->
		<action path="/doWTDevStat" type="com.wtwd.sys.appinterfaces.innerw.WTDevStatAction"></action>		
		
		<!-- 查询iccid 是否为有效的iccid-->
		<action path="/validIccid" type="com.wtwd.sys.appinterfaces.weitechao.validIccidAction"></action>
		 <!-- payfor 支付接口 -->
		<action path="/payforIccid" type="com.wtwd.sys.appinterfaces.weitechao.payforIccidAction"></action>
        <!-- 临时协议，注意不能长期提供，设备定位上报方式接口 -->
		<action path="/doWTDevLCTOpt" type="com.wtwd.sys.appinterfaces.innerw.WTDevLCTOptAction"></action>
		 <!--根据时区来获取套餐价格  -->
		<action path="/byTimeZoneGetMeal" type="com.wtwd.sys.appinterfaces.weitechao.ByTimeZoneGetMealAction"></action>

        <!-- 设置用户首选距离单位 -->
		<action path="/doUDU" type="com.wtwd.sys.appinterfaces.innerw.WTUduAction"></action>		

        <!-- 电子围栏新增或者修改，包括围栏缩略图 -->
		<action path="/doWTSFMn" type="com.wtwd.sys.appinterfaces.innerw.WTSFMnAction"></action>		
        
		<!-- 查询优惠码是否有效 -->
		<action path="/selectPromotionCode" type="com.wtwd.sys.appinterfaces.weitechao.validPromotionCode"></action>
		<!-- 优惠支付 -->
		<action path="/specialPayforIccid" type="com.wtwd.sys.appinterfaces.weitechao.specialPayforIccidAction"></action>
        

        <!-- 内部高德定位接口 -->
		<action path="/doWTGaodeInner" type="com.wtwd.sys.appinterfaces.innerw.WTGaodeInnerAction"></action>		

        
	</action-mappings>
	<message-resources parameter="ApplicationResources" />
</struts-config>

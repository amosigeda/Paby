<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="relativeCallInfo">

	<select id="getRelativeCallInfo" resultClass="com.godoing.rose.lang.DataMap"
		parameterClass="com.wtwd.sys.relativecallinfo.domain.RelativeCallInfo">
		SELECT * FROM relativecallinfo
		<dynamic prepend="where">
			<isNotEmpty prepend="and" property="condition">
				$condition$
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="getMaxCountRelativeCallInfo" resultClass="java.lang.Integer" parameterClass="com.wtwd.sys.relativecallinfo.domain.RelativeCallInfo">
	   select max(id) from relativecallinfo
	</select>
	<select id="getRelativeCallInfoListByVo" resultClass="com.godoing.rose.lang.DataMap"
		parameterClass="com.wtwd.sys.relativecallinfo.domain.RelativeCallInfo">
		SELECT * FROM relativecallinfo
		<dynamic prepend="where">
			<isNotEmpty prepend="and" property="condition">
				$condition$
			</isNotEmpty>
		</dynamic>
		<dynamic prepend="order by">
			<isNotEmpty prepend="order by" property="orderBy">
				$orderBy$
			</isNotEmpty>
		</dynamic>
			limit
			<isNotEmpty prepend=" " property="from">
				$from$
			</isNotEmpty>
				,
			<isNotEmpty prepend=" " property="pageSize">
				$pageSize$
			</isNotEmpty>
	</select>
	
	<select id="getRelativeCallInfoCount" resultClass="java.lang.Integer"
		parameterClass="com.wtwd.sys.relativecallinfo.domain.RelativeCallInfo">
		SELECT COUNT(*) FROM relativecallinfo
		<dynamic prepend="where">
			<isNotEmpty prepend="and" property="condition">
				$condition$
			</isNotEmpty>
		</dynamic>
	</select>

	<insert id="insertRelativeCallInfo"
		parameterClass="com.wtwd.sys.relativecallinfo.domain.RelativeCallInfo">
		INSERT INTO relativecallinfo
		(serie_no,relative_type,phone_number,family_ssid,family_passwd,host_ssid,nick_name,status,user_id,add_time,belong_project) VALUES
		(#serieNo#,#relativeType#,#phoneNumber#,#familySsid#,#familyPasswd#,#hostSsid#,#nickName#,#status#,#userId#,#addTime#,#belongProject#)
	</insert>

	<update id="updateRelativeCallInfoStatus"
		parameterClass="com.wtwd.sys.relativecallinfo.domain.RelativeCallInfo">
		UPDATE relativecallinfo
		<dynamic prepend="set">
			<isNotEmpty prepend="," property="status">
				status=#status#
			</isNotEmpty>
		</dynamic>
		<dynamic prepend="where">
			<isNotEmpty prepend="and" property="serieNo">
				serie_no=#serieNo#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="condition">
				$condition$
			</isNotEmpty>
		</dynamic>
	</update>

	<update id="updateRelativeCallInfo"
		parameterClass="com.wtwd.sys.relativecallinfo.domain.RelativeCallInfo">
		UPDATE relativecallinfo
		<dynamic prepend="set">
			<isNotEmpty prepend="," property="relativeType">
				relative_type=#relativeType#
			</isNotEmpty>
			<isNotEmpty prepend="," property="phoneNumber">
				phone_number=#phoneNumber#
			</isNotEmpty>
			<isNotEmpty prepend="," property="familySsid">
				family_ssid=#familySsid#
			</isNotEmpty>
			<isNotEmpty prepend="," property="familyPasswd">
				family_passwd=#familyPasswd#
			</isNotEmpty>
			<isNotEmpty prepend="," property="hostSsid">
				host_ssid=#hostSsid#
			</isNotEmpty>
			<isNotEmpty prepend="," property="nickName">
				nick_name=#nickName#
			</isNotEmpty>
			<isNotEmpty prepend="," property="status">
				status=#status#
			</isNotEmpty>
		</dynamic>
		<dynamic prepend="where">
			<isNotEmpty prepend="and" property="condition">
				$condition$
			</isNotEmpty>
		</dynamic>
	</update>
	<delete id="deleteRelativeCallInfo" parameterClass="com.wtwd.sys.relativecallinfo.domain.RelativeCallInfo">
		DELETE FROM relativecallinfo
		<dynamic prepend="where">
			<isNotEmpty prepend="and" property="condition">
				$condition$
			</isNotEmpty>
		</dynamic>
	</delete>
</sqlMap>